jota := import("jota")
time := import("time")
log := import("log")

me := jota.Me()
for {
  target := jota.Param("target")
  jota.MoveTowards(target.Pos())
  time.Sleep(30)
  nearby := jota.NearbyEnts()
  if len(nearby) > 0 {
    if nearby[0].Side() != me.Side() && nearby[0].Pos().Sub(me.Pos()).Length() < 50 {
      jota.UseAbility(0, 1.0, true)
    }
  }
}
